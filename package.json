{
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev:web": "turbo run dev --filter=web",
    "dev:app": "turbo run dev --filter=app",
    "dev:auth": "turbo run dev --filter=auth",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "validate": "turbo run validate",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.1.0",
  "author": "Leo <leo@props.to>",
  "name": "props.to",
  "dependencies": {
    "@propsto/tsconfig": "workspace:*",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "turbo": "^1.13.3"
  }
}