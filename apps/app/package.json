{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "next dev",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "next start",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "start-server-and-test dev 3000 \"NODE_ENV=test playwright test\"",
    "ts:check": "tsc --noEmit",
    "dx": "pnpm --filter @ db-setup && pnpm dev",
    "dx:nuke": "pnpm --filter <package> db-nuke && pnpm dx",
    "validate": "pnpm ts:check && pnpm format:check",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.7",
    "@hookform/resolvers": "^3.0.0",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^5.6.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.290.0",
    "next": "^14.0.2",
    "next-auth": "^4.24.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "superjson": "2.2.0",
    "tailwind-merge": "^1.11.0",
    "tailwindcss-animate": "^1.0.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.2",
    "@prisma/nextjs-monorepo-workaround-plugin": "^5.6.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.54.0",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.23",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "workspace:*",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2",
    "vite": "^4.0.0",
    "vitest": "^0.34.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.5.1"
  }
}
